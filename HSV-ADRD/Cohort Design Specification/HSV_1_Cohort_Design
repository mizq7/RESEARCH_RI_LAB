digraph "HSV-1 Cohort Design" {
	subgraph cluster_CohortA {
		label="Cohort A"
		A [label="Condition Occurrence
Patients infected with HSV-1 for the first time"]
		A1 [label="Inclusion Criteria
≥ 1 condition occurrence between 365 days prior to index and 1095 days post-index"]
		A2 [label="Exclusion Criteria
≤ 0 condition occurrence of any excluded diseases between all time prior to index and all time post-index"]
		A3 [label="Observation Period
Event will persist until the end of continuous observation for the condition exposure"]
		A4 [label="Medication Exposure Condition
0 days of medication exposure prior to index date"]
		A5 [label="Observation Period
Medication exposure event will persist until the end of continuous observation"]
		A -> A1
		A -> A2
		A -> A3
		A1 -> A4
		A3 -> A5
	}
